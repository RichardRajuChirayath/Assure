{
    "$schema": "https://railway.app/benchmark.schema.json",
    "build": {
        "builder": "NIXPACKS",
        "buildCommand": "prisma generate && next build"
    },
    "deploy": {
        "startCommand": "npm run start",
        "healthcheckPath": "/api/health",
        "restartPolicyType": "ON_FAILURE"
    }
}